@model IEnumerable<SchoolManagement.Models.Student>
@{
	Layout = "~/Views/Shared/_Layout.cshtml";
	ViewBag.Title = "Students";
}
<table>
 <thead>
  <tr>
   <th>Id</th>
   <th>名字</th>
   <th>主修科目</th>
  </tr>
 </thead>
 <tbody>
		@foreach (var student in Model)
		{
	  <tr>
	   <td>@student.Id</td>
	   <td>@student.Name</td>
	   <td>@student.Major</td>
	  </tr>
		}
 </tbody>
</table>
